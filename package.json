{
  "author": "OctopusDeploy",
  "bugs": {
    "url": "https://github.com/OctopusDeploy/create-release-action/issues"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4"
  },
  "description": "GitHub Action to Create a Relase in Octopus Deploy",
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/tmp": "^0.2.0",
    "@types/urijs": "^1.19.15",
    "@typescript-eslint/parser": "^4.18.0",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.22.0",
    "eslint-plugin-github": "^4.1.2",
    "eslint-plugin-jest": "^24.3.2",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "prettier": "2.2.1",
    "tmp": "^0.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "setupFiles": [
      "<rootDir>/src/test-setup.ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "testRunner": "jest-circus/runner",
    "testTimeout": 10000,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "verbose": true
  },
  "keywords": [
    "cli",
    "deployment",
    "octopus-deploy",
    "github-actions"
  ],
  "main": "dist/index.js",
  "name": "create-release-action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OctopusDeploy/create-release-action.git"
  },
  "scripts": {
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest"
  },
  "version": "1.0.0"
}

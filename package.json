{
  "author": "Octopus Deploy",
  "bugs": {
    "url": "https://github.com/OctopusDeploy/create-release-action/issues"
  },
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.0",
    "tmp": "^0.2.1"
  },
  "description": "GitHub Action to Create a Release in Octopus Deploy",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@types/tmp": "^0.2.1",
    "@typescript-eslint/parser": "^4.28.3",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.30.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "homepage": "https://github.com/OctopusDeploy/create-release-action#readme",
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "setupFiles": [
      "<rootDir>/src/test-setup.ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "testRunner": "jest-circus/runner",
    "testTimeout": 10000,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "verbose": true
  },
  "keywords": [
    "cli",
    "deployment",
    "octopus-deploy",
    "github-actions"
  ],
  "license": "Apache-2.0",
  "main": "out/index.js",
  "name": "create-release-action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OctopusDeploy/create-release-action.git"
  },
  "scripts": {
    "all": "npm run format && npm run build && npm test",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "prebuild": "eslint src/**/*.ts",
    "postbuild": "ncc build -o dist",
    "test": "jest"
  },
  "version": "1.0.2"
}
